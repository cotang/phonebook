{
	"parser": "babel-eslint",
	"rules": {
		"quotes": [
			"error",
			"single"
		],
		"semi": [
			2,
			"always"
		],
		"indent": [
			"error",
			2
		],
		"space-before-function-paren": [
			"error",
			"never"
		],
		"object-curly-spacing": [
			"error",
			"always"
		],
		"no-extra-parens": "error",
		"max-len": [
			"error",
			{
				"code": 100
			}
		],
		"no-multi-spaces": "error"
	}
}